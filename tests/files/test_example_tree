(A + C) + C => B
C => A
B + (C | A) => G

###A + C => B | G
=C
?B

####################-TREE B-#################
#                                           #
#                  Node B                   #
#                        |                  #
#                    node =>                #
#                        |                  #
#            node + -----|------ node C     #
#            ____|____                      #
#            |        |                     #
#        node A     node C                  #
#            |                              #
#        node =>                            #
#            |                              #
#        node C                             #
#                                           #
#############################################


####################-TREE G-#################
#                   Node G                  #
#                       |                   #
#                   node =>                 #
#                       |                   #
#                   node +                  #
#                       |                   #
#       node B -------|------- node '|'     #
#           |               ____|____       #
#           |               |        |      #
#       (TREE B)          node C    node A  #
#                                           #
#############################################